{
  "name": "http-status-code-definitions",
  "version": "1.0.0",
  "description": "Get the definitions for http status codes",
  "main": "./build/index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets es2015,stage-0 -d build/ src/",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha test --  --recursive --compilers js:babel-core/register",
    "coveralls": "npm run compile && npm run coverage && node_modules/.bin/coveralls < coverage/lcov.info",
    "prepublish": "npm run compile",
    "lint": "eslint src",
    "test": "npm run compile && mocha --recursive --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/http-status-code-definition-identifier.git"
  },
  "keywords": [
    "http"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/http-status-code-definition-identifier/issues"
  },
  "homepage": "https://github.com/jaebradley/http-status-code-definition-identifier#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.9.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  },
  "dependencies": {
    "enumify": "^1.0.4",
    "immutable": "^3.8.1"
  }
}
